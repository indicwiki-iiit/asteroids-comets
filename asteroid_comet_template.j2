{%- macro size_attributes(heading, perihelion, aphelion, is_near_earth, is_potentially_hazardous, size_comparison_object, diameter, magnitude, category) -%}
    {{ "\n==భౌతిక లక్షణాలు==\n\n" }}
    {{- "అంతరిక్షంలో ఉండే గ్రహాల కక్ష్య లో సూర్యునికి అన్నిటికంటే దగ్గర స్థానాన్ని పెరిహీలియాన్ (Perihelion), దూరపు స్థానాన్ని ఆఫీలియాన్ (Aphelion) అంటారు." -}}
    {%- set line = [ heading, ' కక్ష్య లో పెరిహీలియాన్ సూర్యుడికి ', perihelion, ' ఆస్ట్రోనామికల్ యూనిట్లు దూరం లో ఉంటుంది. ' ]|join('') -%}
    {{ [ line ] | random }} 
    {%- if is_near_earth == 1 -%}
        {{- "ఈ దూరం 1.4 ఆస్ట్రోనామికల్ యూనిట్ల కంటే తక్కువ ఉండటం వల్ల దీన్ని నియర్ ఎర్త్ ఆబ్జెక్ట్ గా పరిగణిస్తారు. " -}}
    {%- endif -%}
    {%- set line1 = [ 'అలాగే దీని ఆఫీలియాన్ దూరం ', aphelion, ' ఆస్ట్రోనామికల్ యూనిట్లు. ', diameter, ' కిలోమీటర్ల వ్యాసం తో దీని పరిమాణం సుమారు ', size_comparison_object, ' ఉంటుంది. దీని సంపూర్ణ పరిమాణం ([[Absolute Magnitude]]) ', magnitude, '. ']|join('') -%}
        {{ [ line1 ] | random }}
    {{- "\n" -}}
 
    {%- if is_potentially_hazardous == 1 -%}
        {{- "భూమికి దీని కక్ష్య కి దూరం తక్కువ ఉండి, దీని పరిమాణం ఎక్కువగా ఉండటం వల్ల ఇది ప్రమాదకరమైన ([[Potentially Hazardous]]) " -}}
        {%- if 'గ్రహశకలం' in category -%}
            {{- " గ్రహశకలం.\n" -}}
         {%- elif 'తోకచుక్క' in category -%}
            {{- " తోకచుక్క. \n" -}}      
        {%- endif -%}
    
    {%- elif is_potentially_hazardous == 0 -%}
        {{- "భూమి కి దీని కక్ష్యానికి తగినంత దూరం ఉండి, దీని పరిమాణం కూడా చిన్నగా ఉండటం వల్ల ఇది ప్రమాదకరమైన ([[Potentially Hazardous]]) " -}}
        
        {%- if 'గ్రహశకలం' in category -%}
            {{- " గ్రహశకలం కాదు. \n" -}}
         {%- elif 'తోకచుక్క' in category -%}
            {{- " తోకచుక్క కాదు.\n" -}}     
        {%- endif -%}
    {%- endif -%}
{%- endmacro -%}


{%- macro movement_attributes(heading, avg_orbit_speed, orbit_period_days, orbit_period_years, rotation_period) -%}  
    {{"\n==చలనం==\n\n"}}
    {{- "గ్రహాలు వాటి కక్ష్యలో సూర్యుని చుట్టూ తిరగడాన్ని పరిభ్రమణం అంటారు. " -}}
    {%- set line1 = [heading, ' క్షణానికి ', avg_orbit_speed, ' కి.మీ వేగం తో తిరుగుతుంది. దీని కక్ష్య కాలం ', orbit_period_days, ' రోజులు, అంటే సుమారు ', orbit_period_years, ' సంవత్సరాలు.']|join('') -%}
    {{ [ line1 ] | random }}
    {%- if rotation_period != '' -%}
      {{- "గ్రహాలు వాటి అక్షం చుట్టూ అవి తిరగడానికి పట్టే సమయాన్ని భ్రమణ కాలం అంటారు. " -}}
        {%- set line1 = [ 'ఈ గ్రహశకలాని భ్రమణ కాలం సుమారు ', rotation_period, ' గంటలు. ']|join('') -%}
        {%- set line2 = [ heading, ' భ్రమణ కాలం సుమారు ', rotation_period, ' గంటలు. ']|join('') -%}
        {{ [ line1, line2 ] | random }}
    {%- endif -%}
{%- endmacro -%}


{%- macro position_attributes(heading, semi_major_axis, eccentricity, inclination, ascending_node_longitude, periapsis_argument) -%}
    {{"\n==కక్ష్య ఆకారం==\n\n"}}
    {{- "గ్రహాల కక్ష్యలు సాధారణంగా దీర్ఘవృత్త ఆకారంలో ఉంటాయి.  వాటి పొడవు ను సూచించే రేఖని బృహదక్షం (Semi Major Axis) అంటారు. " -}}
    {{- "వికేంద్రత దీర్ఘవృత్తం వక్రతను నిర్ణయిస్తుంది. ఓ గ్రహం వికేంద్రత 0 నుంచి 1 పరిధి లో ఎంత తక్కువ ఉంటే దాని కక్ష్య ఆకారం అంత గుండ్రంగా, వృత్తానికి దగ్గరగా ఉన్నట్లు లెక్క. అలాగే కక్ష్య సమతలానికి భూమధ్యరేఖకు మధ్య ఏర్పడే కోణాన్ని ఇంక్లినేషన్ అంటారు. " -}}
    {%- set line1 = [ heading,  'బృహదక్షం పొడవు ', semi_major_axis, ' ఆస్ట్రోనామికల్ యూనిట్లు,',  ' వికేంద్రత ',  eccentricity, ' ,ఇంక్లినేషన్ ', inclination, ' డిగ్రీలు.']|join('') -%}
    {{ [ line1 ] | random }}
    {{- '\n' -}}
    {{- "ఒక గ్రహం ఉత్తరం నుంచి దక్షిణ సమతలానికి దాటే చోటు ని ఆరోహణ స్థానం ([[Ascending Node]]) అంటారు. హెరిహిలియన్ కి ఆరోహణ స్థానానికి మధ్య ఉండే కోణాన్ని ఆర్గుమెంట్ ఆఫ్ పెరియాప్సిస్ అంటారు. " -}}
    {%- set line2 = [ heading, ' కి దీని రేఖాంశం ', ascending_node_longitude, ' డిగ్రీల దగ్గర ఉండగా, దీని ఆర్గుమెంట్ ఆఫ్ పెరియాప్సిస్ ', periapsis_argument,  ' డిగ్రీలు. ']|join('') -%}
    {{ [ line2 ] | random }}
    {{ '\n' }}
{%- endmacro -%}


{%- macro other_attributes(heading,  spectral_type_tholen, spectral_type_smass) -%}
